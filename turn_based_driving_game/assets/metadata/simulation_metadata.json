{
    "TRAJECTORY_LENGTH": 100,
    "TRAJECTORY_DIMENSIONS": [
        {
            "name": "x",
            "type": "float32"
        },
        {
            "name": "y",
            "type": "float32"
        },
        {
            "name": "yaw",
            "type": "float32"
        },
        {
            "name": "speed",
            "type": "float32"
        },
        {
            "name": "slip_angle",
            "type": "float32"
        },
        {
            "name": "yaw_rate",
            "type": "float32"
        }
    ],
    "params_ranges": {
        "initial_speed": {
            "min": 0,
            "max": 100,
            "step": 10,
            "default": 0,
            "label": "Initial speed"
        },
        "steering": {
            "min": -100,
            "max": 100,
            "step": 2,
            "default": 12,
            "label": "Steering"
        },
        "controls": {
            "type": "exclusive",
            "params": {
                "road_condition": {"default": "dirt"},
                "vehicle_type": {"default": "four_wheel_drive"},
                "throttle": {
                    "min": 0,
                    "max": 100,
                    "step": 10,
                    "default": 0,
                    "label": "Throttle"
                },
                "brake": {
                    "min": 0,
                    "max": 100,
                    "step": 10,
                    "default": 0,
                    "label": "Brake"
                },
                "handbrake": {
                    "min": 0,
                    "max": 100,
                    "step": 100,
                    "default": 0,
                    "label": "Handbrake"
                }
            }
        }
    },
    "view_params": {
        "scale": {
            "default": 3.4,
            "min": 1,
            "max": 10,
            "step": 0.1,
            "label": "Scale"
        }
    },
    "file_pattern": "initial_speed{initial_speed}_steering{steering}_controlsALL.dat",
    "base_url": "/turn-based-driving-game/data/",
    "file_params": ["initial_speed", "steering"],
    "offset_params": ["throttle"]
}